{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "no-console": "off",
        "indent": "off",
        "comma-dangle": "off",
        "semi": "off",
        "linebreak-style":"off",
        "no-underscore-dangle":"off",
        "import/no-extraneous-dependencies": "off"
    }
}
